
<div>
  <video id="remoteVideo" autoplay></video>
  <video id="localVideo" autoplay></video>
  <video id="remote2Video"></video>
</div>
<div class="input">
  <label for="channel_val"> channel</label><br>
  <input id="channel_val" value="101010101"></input>
</div>

<div id="actions">
  <button id="connectButton">Connect</button>
  <button id="disconnectButton" disabled>Disconnect</button>
  <button id="callButton" disabled>Call</button>
  <button id="call2Button" disabled>Call2</button>
  </div>
</div>
<div id="record-actions">
  <button id="recordButton">Start Recording</button>
  <button id="playButton" disabled>playButton</button>
  <button id="downloadButton" disabled>downloadButton</button>
  </div>
</div>

<% @files.each do |name| %>
  <%= video_tag(name, controls: true, loop: true, 'webkit-playsinline': true, id: name, type: 'video/webm', height: 200) %>
<%end%>


